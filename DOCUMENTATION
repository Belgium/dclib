DOCUMENTATION

player.lua

    Player(id)
        returns a player object

    Player(table)
        converts a table of player ids to a table of player objects

    Player.table
        a table with all players

    Player.tableliving
        a table with all living players

    Player.team1
        a table with all terrorists

    Player.team2
        a table with all counter-terrorists

    Player.team1living
        a table with all living terrorists

    Player.team2living
        a table with all living counter-terrorists

    methods
        equip(weapon)
        setpos(x, y)
        reqcld(mode, [parameter])
        kick(reason)
        banip(duration, reason)
        banusgn(duration, reason)
        spawn()
        strip(wpn)
        slap()
        kill()
        customkill(killer, weapon)
        msg(message)
        cmsg(message)
        shake(power)
        ai_aim(x,y)
        ai_attack(secondary)
        ai_build(building, x, y)
        ai_buy(itemtype)
        ai_debug(text)
        ai_dropt()
        ai_findtarget()
        ai_freeline(x, y)
        ai_goto(x, y, walk)
        ai_iattack()
        ai_move(angle, walk)
        ai_radio(radioid)
        ai_reload()
        ai_respawn()
        ai_rotate(angle)
        ai_say(text)
        ai_sayteam(text)
        ai_selectweapon(itemtype)
        ai_spray()
        ai_use()

    variables
        name
        team
        x
        y
        tilex
        tiley
        health
        armor
        money
        score
        deaths
        speedmod
        maxhealth
        weapon
        weapons

    read-only variables
        exists
        enemy
        ip
        port
        usgn
        ping
        idle
        bot
        look
        rot
        teamkills
        hostagekills
        teambuildingkills
        weapon
        nightvision
        defusekit
        gasmask
        bomb
        flag
        reloading
        process
        sprayname
        spraycolor
        votekick
        votemap
        favteam
        rcon
        ai_flash


image.lua

    Image(path, x, y, mode, [pl])
        returns an image object
    
    methods
        remove()
        alpha(alpha)
        blend(mode)
        color(red, green, blue)
        hitzone(mode, x, y, w, h)
        pos(x, y, rot)
        scale(x, y)
        t_alpha(time, alpha)
        t_color(time, red, green, blue)
        t_move(time, x, y, [rot])
        t_rotate(time, rot)
        t_rotateconstantly(speed)
        t_scale(time, x, y)
    
    read-only variables
        id


object.lua

    Object(id)
        returns an object

    Object(table)
        converts a table of object ids to a table of objects

    Object(name)
        returns corresponding type id for the name

    Object.table
        a table with all objects
    
    Object.spawn(type, x, y, rot, mode, team, player)
        spawns a new object
    
    methods
        :kill()

    read-only variables
        id
        exists
        typename
        type
        health
        mode
        team
        player
        x
        y
        rot
        tilex
        tiley
        countdown
        rootrot
        idle
        rotvar
        target
        upgrade
        entity
        entityx
        entityy


hook.lua

    Hook.add(hook, func, [priority])
        creates a hook which uses lapi objects instead of ids
        returns Hook object

    methods
        remove()

    variables
        id
        hook
        func
        prio
